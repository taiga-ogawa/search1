#!/bin/bash

echo "Content-type: text/html" #httpが解釈する場所、おまじない(あとで勉強)ー＞httpヘッダーにこれから書くのはhtmlで書かれたwebページだよって言ってる
echo
	echo "<table border ="1" align ="center">"
	     echo "<tr>"
	     	  echo "<th>"
	     	       echo "検索結果"
	     	  echo "</th>"
		  echo "<th>"
	     	       echo "類似度"
	          echo "</th>"
	   	  echo "<th>"
	     	       echo "title"
	     	  echo "</th>"
		  echo "<th>"
		       echo "author"
		  echo "</th>"
	     echo "</tr>"
array1=("2" "5" "5")
array2=("3" "4" "7")
echo "$((array1[0]*array2[0] + array1[1]*array2[1] + array1[2]*array2[2]))"
 naiseki 0.2 0.5 0.5 0.3 0.4 0.7
 ./a.out a b c